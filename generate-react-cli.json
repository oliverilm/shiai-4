{
    "usesTypeScript": true,
    "usesCssModule": true,
    "cssPreprocessor": "scss",
    "testLibrary": "Testing Library",
    "component": {
      "default": {
        "path": "src/components",
        "withLazy": false,
        "withStory": false,
        "withStyle": true,
        "withTest": true
      },
      "page": {
        "customTemplates": {
            "component": "templates/page/TemplateName.tsx"
          },
        "path": "src/views",
        "withLazy": true,
        "withStory": false,
        "withStyle": true,
        "withTest": true
      },
      "layout": {
        "path": "src/layout",
        "withLazy": false,
        "withStory": false,
        "withStyle": false,
        "withTest": true
      }
    }
  }